{project_app_dirs, ["exercises/*"]}.

{plugins,
 [{'lfe-compile',
   {git, "git://github.com/lfe-rebar3/compile.git",
    {tag, "0.4.0"}}}]}.

{provider_hooks,
 [{pre,  [{eunit,   {lfe, compile}}]},
  {post, [{compile, {lfe, compile}}]}]}.

{deps,
 [{lfe,
   {git, "git://github.com/rvirding/lfe.git",
    {tag, "1.0.2"}}}]}.

{profiles,
 [{test,
   [{deps,
     [{ltest,
       {git, "git://github.com/lfex/ltest.git",
        {tag, "0.8.2"}}}]}]}]}.
